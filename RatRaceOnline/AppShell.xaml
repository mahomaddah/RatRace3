<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RatRace3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RatRace3"

    Title="RatRace3">

    
    <ShellContent
      Title="Home"
      ContentTemplate="{DataTemplate local:MainPage}"
      Route="MainPage" />
    
    <FlyoutItem Title="Market Page">
        <ShellContent Title="Market Page" ContentTemplate="{DataTemplate local:MarketPage}"  
      Route="MarketPage"/>
    </FlyoutItem>

    <ShellContent Title="" ContentTemplate="{DataTemplate local:MarketPage}"  
    Route="MarketPage"/>

</Shell>
